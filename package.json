{
  "private": true,
  "name": "sf-scaffolding-api",
  "version": "0.0.1",
  "description": "SF scaffolding api",
  "engines": {
    "node": "14",
    "npm": "6"
  },
  "engineStrict": true,
  "scripts": {
    "postinstall": "npm run compile:all",
    "compile:all": "rm -rf dist && npm run compile:openapi && npm run compile:ts",
    "compile:ts": "tsc -p tsconfig.src.json",
    "compile:openapi": "ts-node npm_scripts/compile-openapi.ts",
    "eslint": "eslint '**/*.{ts,js}' --ignore-pattern node_modules/; exit 0",
    "eslint:fix": "eslint '**/*.{ts,js}' --fix --ignore-pattern node_modules/; exit 0",
    "test": "mocha -r ts-node/register \"test/**\"",
    "live": "concurrently -n \"src,openapi\" \"npm run watch:src\" \"npm run watch:openapi\"",
    "watch:src": "rm -rf dist && tsc-watch --noClear -p tsconfig.src.json --onSuccess \"node dist/index.js\"",
    "watch:openapi": "nodemon --watch openapi --exec 'ts-node' npm_scripts/compile-openapi.ts"
  },
  "keywords": [],
  "main": "dist/index.js",
  "author": "Soluzioni Futura <info@soluzionifutura.it>",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.7",
    "@soluzioni-futura/config-component": "^3.0.5",
    "@soluzioni-futura/docs-helper": "^1.0.9",
    "@soluzioni-futura/eslint-config-soluzioni-futura": "^2.0.0",
    "@soluzioni-futura/extended-error": "^1.0.2",
    "@soluzioni-futura/openapi-helper": "^1.0.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.10.9",
    "json-schema": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "statuses": "^2.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.19",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/express-session": "^1.17.3",
    "@types/fs-extra": "^9.0.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/statuses": "^2.0.0",
    "@types/tsc-watch": "^4.2.0",
    "@types/tunnel-ssh": "^4.1.0",
    "@types/uuid": "^8.3.1",
    "@types/yargs-parser": "^20.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "ajv": "^6.12.6",
    "chai": "^4.3.4",
    "concurrently": "^5.3.0",
    "dotenv": "^8.6.0",
    "eslint": "^7.19.0",
    "fs-extra": "^9.1.0",
    "json-schema-to-typescript": "^10.1.3",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "openapi-typescript-codegen": "^0.8.1",
    "ts-node": "^10.0.0",
    "tsc-watch": "^4.2.9",
    "tunnel-ssh": "^4.1.4",
    "typescript": "^4.3.5",
    "yargs-parser": "^20.2.4"
  }
}
